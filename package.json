{
    "name": "vuxel",
    "private": false,
    "version": "0.2.1",
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/vuxel.umd.cjs",
    "module": "./dist/vuxel.js",
    "exports": {
        ".": {
            "import": "./dist/vuxel.js",
            "require": "./dist/vuxel.umd.cjs"
        },
